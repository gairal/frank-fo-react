{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true
    },
    "plugins": [
      "babel",
      "react",
      "promise"
    ],
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": 0,
        "curly": 2,
        "indent": [2, 2, { "SwitchCase": 1 }],
        "eqeqeq": 2,
        "new-cap": 2,
        "camelcase": 2,
        "quotes": [2, "single"],
        "no-trailing-spaces": 2,
        "no-console": 1
    }
}
