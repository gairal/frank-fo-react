{
  "presets": [
    "react",
    [ "env", {
      "targets": {
        "browsers": "> 2%"
      }
    }],
    "stage-0"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "lodash",
    ["transform-imports", {
        "@material-ui/core": {
            "transform": "@material-ui/core/${member}",
            "preventFullImport": true
        },
        "react-router-dom": {
            "transform": "react-router-dom/es/${member}",
            "preventFullImport": true
        },
        "redux": {
            "transform": "redux/es/${member}",
            "preventFullImport": true
        },
        "react-google-maps/lib/components": {
            "transform": "react-google-maps/lib/components/${member}",
            "preventFullImport": true
        },
        "react-google-maps/lib": {
            "transform": "react-google-maps/lib/${member}",
            "preventFullImport": true
        }
    }]
]
}
