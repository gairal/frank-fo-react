{
  "name": "frank-gairal-fo-react",
  "version": "1.0.0",
  "description": "Front-end Stack with Jade, Webpack, Babel, ES2015, SASS",
  "author": "Gairal",
  "readmeFilename": "README.md",
  "main": "src/app/app.js",
  "keywords": [
    "Front-end",
    "Jade",
    "Sass",
    "css",
    "JavaScript",
    "Webpack",
    "babel",
    "es6",
    "es2015"
  ],
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --config ./webpack/webpack.config.js --hot",
    "startprod": "./node_modules/.bin/webpack-dev-server --config ./webpack/webpack.config.serveprod.js --hot",
    "debug": "./node_modules/.bin/webpack-dev-server --config ./webpack/webpack.config.debug.js --hot",
    "build": "./node_modules/.bin/webpack --config ./webpack/webpack.config.js",
    "release": "./node_modules/.bin/webpack --config ./webpack/webpack.config.prod.js",
    "lint": "./node_modules/.bin/eslint ./src/ && ./node_modules/.bin/eslint ./test/ && ./node_modules/.bin/stylelint './src/**/*.scss'",
    "test": "NODE_ENV=testing ./node_modules/.bin/jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all lint test",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^7.0.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-imports": "^1.5.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-alias": "^1.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.9",
    "imagemin-webpack-plugin": "^2.1.5",
    "jest": "^23.1.0",
    "jest-fetch-mock": "^1.6.5",
    "jest-localstorage-mock": "^2.2.0",
    "jest-mock-console": "^0.3.5",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "react-test-renderer": "^16.4.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylelint-config-concentric-order": "^3.0.0",
    "stylelint-config-sass-guidelines": "^5.0.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.1.3",
    "stylelint-selector-bem-pattern": "^2.0.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "uglifyjs-webpack-plugin": "^1.2.6",
    "webpack": "^4.12.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "write-file-webpack-plugin": "^4.3.2"
  },
  "dependencies": {
    "@material-ui/core": "^1.2.3",
    "whatwg-fetch": "^2.0.4",
    "es6-promise": "^4.2.4",
    "font-awesome": "^4.7.0",
    "react-google-maps": "^9.4.5",
    "react": "^16.4.1",
    "redux": "^3.7.2",
    "prop-types": "^15.6.2",
    "react-dom": "^16.4.1",
    "react-hot-loader": "~4.3.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux-thunk": "^2.3.0"
  }
}
