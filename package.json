{
  "name": "frank-gairal-fo-react",
  "version": "1.0.0",
  "description": "Front-end Stack with Jade, Webpack, Babel, ES2015, SASS",
  "author": "Gairal",
  "readmeFilename": "README.md",
  "main": "src/app/app.js",
  "keywords": [
    "Front-end",
    "Jade",
    "Sass",
    "css",
    "JavaScript",
    "Webpack",
    "babel",
    "es6",
    "es2015"
  ],
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --config ./webpack/webpack.config.js",
    "startprod": "./node_modules/.bin/webpack-dev-server --config ./webpack/webpack.config.serveprod.js",
    "debug": "./node_modules/.bin/webpack-dev-server --config ./webpack/webpack.config.debug.js",
    "build": "./node_modules/.bin/webpack --config ./webpack/webpack.config.js",
    "release": "./node_modules/.bin/webpack --config ./webpack/webpack.config.prod.js",
    "lint": "./node_modules/.bin/eslint ./src/ && ./node_modules/.bin/eslint ./test/ && ./node_modules/.bin/stylelint './src/**/*.scss'",
    "test": "NODE_ENV=testing ./node_modules/.bin/jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint & npm test",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.1",
    "@babel/preset-env": "^7.1",
    "@babel/plugin-proposal-class-properties": "^7.1",
    "@babel/preset-react": "^7.0",
    "@commitlint/cli": "^7.2",
    "@commitlint/config-conventional": "^7.1",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0",
    "babel-jest": "^23.6",
    "babel-loader": "^8.0",
    "babel-plugin-lodash": "^3.3",
    "babel-plugin-transform-imports": "^1.5",
    "clean-webpack-plugin": "^0.1",
    "copy-webpack-plugin": "^4.6",
    "css-loader": "^1.0",
    "eslint": "^5.8",
    "eslint-config-airbnb": "^17.1",
    "eslint-config-prettier": "^3.1",
    "eslint-import-resolver-alias": "^1.1",
    "eslint-loader": "^2.1",
    "eslint-plugin-import": "^2.14",
    "eslint-plugin-jest": "^21.27",
    "eslint-plugin-jsx-a11y": "^6.1",
    "eslint-plugin-prettier": "^3.0",
    "eslint-plugin-react": "^7.11",
    "file-loader": "^2.0",
    "html-webpack-plugin": "^3.2",
    "husky": "^1.1",
    "imagemin-webpack-plugin": "^2.3",
    "jest": "^23.6",
    "jest-fetch-mock": "^1.6",
    "jest-localstorage-mock": "^2.3",
    "jest-mock-console": "^0.4",
    "mini-css-extract-plugin": "^0.4",
    "node-sass": "^4.10",
    "prettier": "^1.14",
    "pug": "^2.0",
    "pug-loader": "^2.4",
    "react-test-renderer": "^16.6",
    "sass-loader": "^7.1",
    "style-loader": "^0.23",
    "stylelint-config-concentric-order": "^3.4",
    "stylelint-config-sass-guidelines": "^5.2",
    "stylelint-order": "^1.0",
    "stylelint-scss": "^3.3",
    "stylelint-selector-bem-pattern": "^2.0",
    "stylelint-webpack-plugin": "^0.10",
    "uglifyjs-webpack-plugin": "^2.0",
    "webpack": "^4.25",
    "webpack-bundle-analyzer": "^3.0",
    "webpack-cli": "^3.1",
    "webpack-dev-server": "^3.1",
    "write-file-webpack-plugin": "^4.4"
  },
  "dependencies": {
    "@material-ui/core": "^1.2",
    "whatwg-fetch": "^3.0",
    "es6-promise": "^4.2",
    "font-awesome": "^4.7",
    "react-google-maps": "^9.4",
    "react": "^16.6",
    "redux": "^4.0",
    "prop-types": "^15.6",
    "react-dom": "^16.6",
    "react-hot-loader": "~4.3",
    "react-redux": "^5.1",
    "react-router-dom": "^4.3",
    "redux-thunk": "^2.3"
  }
}
